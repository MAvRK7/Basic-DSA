#include <stdio.h>

int main()
{
	int n;
	printf("number\n");
	scanf("%d", &n);
	for (int i = 0; i < n ; i++)
	{
		for (int j = 0; j <= i; j++)
		{
			if ((i + j) % 2 == 0)
			{
				printf("1");
			}
			else
			{
				printf("0");
			}
		}
		printf("\n");
	}

	return 0;
}

//This is from the midsem Q1c, it gives a left aligned triangle on 1 and 0 pattern 
